{
  "compilerOptions": {
    "allowJs": true,
    "noEmit": true,
    "esModuleInterop": true,
    "target": "ESNext",
    "moduleResolution": "node",
    "preserveSymlinks": false,
    "strictNullChecks": true,
    "strictBindCallApply": true,
    "strictFunctionTypes": true
  },
  "include": ["**/*.js"],
  "exclude": ["node_modules"]
}